{
  "name": "react-selenium-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chromedriver": "^123.0.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "selenium-webdriver": "^4.15.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:e2e": "mocha \"e2e/**/*.js\" --timeout 60000 --reporter mocha-junit-reporter"
  },
  "mocha-junit-reporter": {
    "mochaFile": "test-results/mocha-results.xml",
    "suiteTitleSeparatedBy": " / "
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
